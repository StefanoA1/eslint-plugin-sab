{
  "name": "@stefanoa1/eslint-plugin-sab",
  "version": "3.1.0",
  "description": "Common ESLint config for Personal projects",
  "homepage": "https://github.com/StefanoA1/eslint-plugin-sab#readme",
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StefanoA1/eslint-plugin-sab.git"
  },
  "author": "Stefano Acosta <stefanoacosta.92@gmail.com>",
  "contributors": [
    "Stefano Acosta <stefanoacosta.92@gmail.com> (https://github.com/StefanoA1/)"
  ],
  "bugs": {
    "url": "https://github.com/StefanoA1/eslint-plugin-sab/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "eslint-ast-utils": "^1.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "~7.34.2",
    "eslint-plugin-unicorn": "^49.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "c8": "^9.1.0",
    "esbuild": "0.21.4",
    "eslint": "^8.57.0",
    "eslint-ava-rule-tester": "^4.2.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-self": "^1.2.1"
  },
  "overrides": {
    "cross-spawn": "^7.0.5"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && esbuild index.js --minify-whitespace --minify-syntax --sourcemap --bundle --platform=node --target=node24 --outfile=dist/index.js",
    "test": "npm run lint && npm run test:unit && npm run find-missing",
    "test:unit": "c8 ava",
    "find-missing": "node scripts/find-missing",
    "prettier-core": "prettier --write src/config/**/*.js src/rules/**/*.js test/**/*.js *.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  }
}
